{
  "name": "electron-updater-example",
  "version": "0.7.3",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "publish": "build -p always"
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1"
  },
  "dependencies": {
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5"
  },
  "build": {
    "appId": "com.github.iffy.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
              "ia32"
          ]
        },
        {
            "target": "portable",
            "arch": [
                "ia32"
            ]
        },
        {
            "target": "zip",
            "arch": [
                "ia32"
            ]
        }
      ]
    },
    "publish": {
      "provider": "github"
    }
  }
}
